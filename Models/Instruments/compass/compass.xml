<?xml version="1.0"?>
<PropertyList>

 <path>compass.ac</path>
 
<animation>
     <type>material</type>
     <object-name>Compass</object-name>
     <emission>
         <red>0.035</red>
         <green>0.012</green>
         <blue>0.007</blue>
         <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
     </emission>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>Compass</object-name>
  <property>/instrumentation/magnetic-compass/indicated-heading-deg</property>
  <center>
   <x-m>-0.027</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

</PropertyList>


