	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>BeaconOffX</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep alias="../../../../BeaconOffXparams/light-near"/>
			</entry>
			<entry>
				<ind>500</ind>
				<dep alias="../../../../BeaconOffXparams/light-med"/>
			</entry>
			<entry>
				<ind>16000</ind>
				<dep alias="../../../../BeaconOffXparams/light-far"/>
			</entry>
		</interpolation>
	</animation>
	
