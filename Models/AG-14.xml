    <!-- Animaciones del fuselaje -->

 <animation>
  <type>noshadow</type>
  <object-name>propblur</object-name>
  <object-name>propdisc</object-name>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Propeller</object-name>
  <condition>
   <less-than>
    <property>engines/engine/rpm</property>
    <value>299</value>
   </less-than>
  </condition>
 </animation>
 
   <animation>
        <type>select</type>
        <object-name>propblur</object-name>
        <condition>
            <greater-than>
                <property>engines/engine/rpm</property>
                <value>300</value>
            </greater-than>
            <less-than>
                <property>engines/engine/rpm</property>
                <value>3000</value>
            </less-than>
        </condition>
  </animation>

  <animation>
        <type>spin</type>
        <object-name>propblur</object-name>
        <property>engines/engine/rpm</property>
        <center>
          <x-m>1.16186</x-m>
          <y-m>0.0042989</y-m>
          <z-m>-0.0269645</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
  </animation>

 <animation>
        <type>select</type>
        <object-name>propdisc</object-name>
        <condition>
            <greater-than>
                <property>engines/engine/rpm</property>
                <value>3001</value>
            </greater-than>
        </condition>
 </animation>

<animation>
        <type>spin</type>
        <object-name>propdisc</object-name>
        <property>engines/engine/rpm</property>
        <factor>0.005</factor>
        <center>
         <x-m>1.16186</x-m>
         <y-m>0.0042989</y-m>
         <z-m>-0.0269645</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>Propeller</object-name>
  <property>engines/engine[0]/rpm</property>
  <factor>-1</factor>
