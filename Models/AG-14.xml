	</animation>

	<animation>
		<type>flash</type>
		<object-name>landinglight</object-name>
		<center>
			<x-m>-2.80714</x-m>
			<y-m>-0.00139999</y-m>
			<z-m>-0.40508</z-m>
		</center>
